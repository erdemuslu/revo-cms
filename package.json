{
  "name": "revo-cms",
  "version": "1.0.0",
  "description": "A basic cms to create your own blog",
  "main": "index.js",
  "scripts": {
    "client:watch": "parcel watch dashboard/app/main.js -d dashboard/output --no-source-maps",
    "client:build": "parcel build dashboard/app/main.js -d dashboard/output --no-source-maps",
    "client:clean": "rm -rf dashboard/output",
    "server:dev": "pm2 start dev.ecosystem.config.js --env development",
    "server:prod": "pm2 start ecosystem.config.js --env production"
  },
